            <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Generate Date" enabled="true">
              <stringProp name="BeanShellSampler.query">import java.text.DateFormat;
import java.text.DateFormat;
import java.text.SimpleDateFormat;

String randomDate = &quot;${__RandomDate(dd MM yyyy,${CreationDateDown},${CreationDateUp},en_EN,)}&quot;;
DateFormat formatTo = new SimpleDateFormat(&quot;dd MM yyyy&quot;, Locale.ENGLISH);
Date date = formatTo.parse(randomDate);

var dateString = new SimpleDateFormat(&quot;yyyy-MM-dd&quot;).format(date);
var hours = Integer.parseInt(&quot;${__Random(0,23)}&quot;);
var minutes = Integer.parseInt(&quot;${__Random(0,59)}&quot;);
var formatedHours = String.format(&quot;%02d&quot;, new Object[] {hours}); 
var formatedMinutes = String.format(&quot;%02d&quot;, new Object[] {minutes}); 

var creationDate = dateString + &quot; &quot; + formatedHours + &quot;:&quot; + formatedMinutes;
print(creationDate);
vars.put(&quot;CreationDate&quot;, creationDate);

</stringProp>
              <stringProp name="BeanShellSampler.filename"></stringProp>
              <stringProp name="BeanShellSampler.parameters"></stringProp>
